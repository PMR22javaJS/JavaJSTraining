class Heart{
	void pumping() {
		System.out.println("Heart is Pumping .....");
	}
}

class Human{
	private Heart heart=new Heart();
	void breathing() {
		System.out.println("Human is breathing ....");
		heart.pumping();
	}
}

public class HasATest {
	public static void main(String[] args) {
		Human human =new Human();
		human.breathing();
	}
}
