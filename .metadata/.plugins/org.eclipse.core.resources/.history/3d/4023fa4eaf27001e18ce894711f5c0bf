
class Doctor{
	void diagnose() {
		System.out.println("Go for diagnosis");
	}
}

class Surgeon extends Doctor{
	void cut() {
		System.out.println("Cut the bacterial part");
	}
	void stitch() {
		System.out.println("Stitch the organ");
	}
}	

class HeartSurgeon extends Surgeon{
	
}

