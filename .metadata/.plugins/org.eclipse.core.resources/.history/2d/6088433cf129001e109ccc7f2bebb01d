class Heart{
	void pumping() {
		System.out.println("Heart is Pumping .....");
	}
}

class Brain{
	void analyzing() {
		System.out.println("Brain is analyzing");
	}
}

class Kidney{
	void filtering() {
		System.out.println("Kidney is filtering");
	}
}

class Human{
	private Heart heart=new Heart();						//hasA 
	private Brain brain=new Brain();						//hasA relation
	private Kidney kidney=new Kidney();
	void breathing() {
		System.out.println("Human is breathing ....");
		heart.pumping();
		brain.analyzing();
		kidney.filtering();
	}
}

public class HasATest {
	public static void main(String[] args) {
		Human human =new Human();
		human.breathing();
	}
}
